                                <li>
                                    <a href="@Url.Action("List", "Sales")"><i class="fa fa-table"></i>Продажи</a>
                                </li>
                                <li>
                                    <a href="@Url.Action("List", "Incubators")"><i class="fa fa-table"></i>Инкубаторы</a>
                                </li>
                            </ul>
